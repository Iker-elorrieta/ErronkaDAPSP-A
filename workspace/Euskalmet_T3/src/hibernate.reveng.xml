<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
	</type-mapping>


	<table-filter match-name="usuario" match-catalog="euskomet_db"
		exclude="true">
	</table-filter>
	<table-filter match-name="fav_municipio" match-catalog="euskomet_db"
		exclude="true">
	</table-filter>
	<table-filter match-name="fav_espacios" match-catalog="euskomet_db"
		exclude="true">
	</table-filter>
	<table-filter match-name="hash" match-catalog="euskomet_db"
		exclude="true">
	</table-filter>
	<table catalog="euskomet_db" name="calidad_aire">
		<column name="fecha_hora"></column>
		<column name="cod_estacion"></column>
		<column name="calidad"></column>
	</table>
	<table catalog="euskomet_db" name="espacios_naturales">
		<column name="cod_enatural"></column>
		<column name="tipo"></column>
		<column name="latitud"></column>
		<column name="nombre"></column>
		<column name="descripcion"></column>
		<column name="foto"></column>
		<column name="longitud"></column>
	</table>
	<table catalog="euskomet_db" name="estaciones">
		<column name="cod_estacion"></column>
		<column name="coord_y"></column>
		<column name="longitud"></column>
		<column name="cod_muni"></column>
		<column name="latitud"></column>
		<column name="coord_x"></column>
		<column name="nombre"></column>
		<column name="direccion"></column>
	</table>
	<table catalog="euskomet_db" name="muni_espacios">
		<column name="cod_muni"></column>
		<column name="cod_enatural"></column>
	</table>
	<table catalog="euskomet_db" name="municipio">
		<column name="cod_muni"></column>
		<column name="foto"></column>
		<column name="descripcion"></column>
		<column name="cod_prov"></column>
		<column name="nombre"></column>
	</table>
	<table catalog="euskomet_db" name="provincia">
		<column name="cod_prov"></column>
		<column name="nombre"></column>
	</table>
</hibernate-reverse-engineering>